<!DOCTYPE html>
<html lang="en">
@using NFD.BLL
@using COM.Utility
@{
    var current_theme = CookiesHelper.GetCookieValue("current_theme");
    if (current_theme == null || current_theme==""){
        current_theme = "classic";
    }
    }
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    @PubMethod.GetLeftMenuJson
    <!-- jQuery -->
    <script src="/Content/Scripts/Plugs/Single/jquery-1.9.1.js" type="text/javascript"></script>
    <link rel="stylesheet" href="/Content/Scripts/Plugs/Multiple/zTree-zTree_v3-master/zTree_v3/css/demo.css"
        type="text/css">
    <link rel="stylesheet" href="/Content/Scripts/Plugs/Multiple/zTree-zTree_v3-master/zTree_v3/css/zTreeStyle/zTreeStyle.css"
        type="text/css">
    <script type="text/javascript" src="/Content/Scripts/Plugs/Multiple/zTree-zTree_v3-master/zTree_v3/js/jquery.ztree.core-3.5.js"></script>
    <link href="/Content/Css/layout.css" rel="stylesheet" type="text/css" />
    @RenderSection("head", false)
    <script src="/Content/Scripts/View/layout.js" type="text/javascript"></script>
    <script src="/Content/Scripts/View/SelectBind.js" type="text/javascript"></script>
    @*<script src="/Content/Scripts/Plugs/Single/ejq.js" type="text/javascript"></script>*@
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Charisma, a fully featured, responsive, HTML5, Bootstrap admin template.">
    <meta name="author" content="Muhammad Usman">
    <!-- The styles -->
    <link href="/Content/Css/jqgrid.css" rel="stylesheet" type="text/css" />
    <link id="bs-css" href="/Content/Templates/usmanhalalit-charisma-d29440e/css/bootstrap-@(current_theme).css"
        rel="stylesheet">
    <link href="/Content/Templates/usmanhalalit-charisma-d29440e/css/bootstrap-responsive.css"
        rel="stylesheet">
    <link href="/Content/Templates/usmanhalalit-charisma-d29440e/css/charisma-app.css"
        rel="stylesheet">
    <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script type="text/javascript" src="/Content/Scripts/Plugs/Single/html5.js"></script>
	<![endif]-->
    <!-- The fav icon -->
    <link rel="shortcut icon" href="/Content/Templates/usmanhalalit-charisma-d29440e/img/favicon.ico">
</head>
<body>
    <!-- topbar starts -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar">
                    </span></a><a class="brand" href="/">
                        <img alt="Charisma Logo" src="/Content/Templates/usmanhalalit-charisma-d29440e/img/logo20.png" />
                        <span>@AppstringManager.GetSystemName</span></a>
                <!-- theme selector starts -->
                <div class="btn-group pull-right theme-container">
                    <a class="btn dropdown-toggle" data-toggle="dropdown" href="#"><i class="icon-tint">
                    </i><span class="hidden-phone">皮肤</span> <span class="caret"></span></a>
                    <ul class="dropdown-menu" id="themes">
                        <li><a data-value="classic" href="#"><i class="icon-blank"></i>传统</a></li>
                        <li><a data-value="cerulean" href="#"><i class="icon-blank"></i>蔚蓝</a></li>
                        <li><a data-value="cyborg" href="#"><i class="icon-blank"></i>电子</a></li>
                        <li><a data-value="redy" href="#"><i class="icon-blank"></i>现成</a></li>
                        <li><a data-value="journal" href="#"><i class="icon-blank"></i>日报</a></li>
                        <li><a data-value="simplex" href="#"><i class="icon-blank"></i>单纯</a></li>
                        <li><a data-value="slate" href="#"><i class="icon-blank"></i>板岩 </a></li>
                        <li><a data-value="spacelab" href="#"><i class="icon-blank"></i>空间</a></li>
                        <li><a data-value="united" href="#"><i class="icon-blank"></i>统一</a></li>
                    </ul>
                </div>
                <!-- theme selector ends -->
                <!-- user dropdown starts -->
                <div class="btn-group pull-right">
                    <a class="btn dropdown-toggle" data-toggle="dropdown" href="#"><i class="icon-user">
                    </i><span class="hidden-phone">@UserManager.GetCurrentUserInfo.userName</span> <span
                        class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li class="divider"></li>
                        <li><a href="/user/Logout">注销</a></li>
                    </ul>
                </div>
                <!-- user dropdown ends -->
                <div class="top-nav nav-collapse">
                    <ul class="nav">
                       @* <li><a href="#">搜 索</a></li>
                        <li>
                            <form class="navbar-search pull-left">
                            <input placeholder="请输入按回车" class="search-query span2" name="query" type="text">
                            </form>
                        </li>*@
                    </ul>
                </div>
                <!--/.nav-collapse -->
            </div>
        </div>
    </div>
    <!-- topbar ends -->
    <div class="container-fluid">
        <div class="row-fluid">
            <!-- left menu starts -->
            <div class="main-menu-span" style=" width:170px">
                <div class="zTree_v3 zTreeDemoBackground left">
                    <ul id="divLayoutMenu" class="ztree">
                    </ul>
                </div>
                <!--/.well -->
            </div>
            <!--/span-->
            <!-- left menu ends -->
            @RenderSection("bodytop", false)
            <div id="content" class="span10">
                @RenderBody()
            </div>
        </div>
        <hr />
        <div class="modal hide fade" id="myModal">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    ×</button>
                <h3>
                    Settings</h3>
            </div>
            <div class="modal-body">
                <p>
                    Here settings can be configured...</p>
            </div>
            <div class="modal-footer">
                <a href="#" class="btn" data-dismiss="modal">Close</a> <a href="#" class="btn btn-primary">
                    Save changes</a>
            </div>
        </div>
        <footer>
           
        </footer>
    </div>
    <!--/.fluid-container-->
    <!-- external javascript ==================================================
    -->
    <!-- Placed at the end of the document so the pages load faster -->
    <!-- jQuery
    UI -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery-ui-1.8.21.custom.min.js"></script>
    <!-- transition / effect library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-transition.js"></script>
    <!-- alert enhancer library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-alert.js"></script>
    <!-- modal / dialog library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-modal.js"></script>
    <!-- custom dropdown library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-dropdown.js"></script>
    <!-- scrolspy library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-scrollspy.js"></script>
    <!-- library for creating tabs -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-tab.js"></script>
    <!-- library for advanced tooltip -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-tooltip.js"></script>
    <!-- popover effect library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-popover.js"></script>
    <!-- button enhancer library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-button.js"></script>
    <!-- accordion library (optional, not used in demo) -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-collapse.js"></script>
    <!-- carousel slideshow library (optional, not used in demo) -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-carousel.js"></script>
    <!-- autocomplete library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-typeahead.js"></script>
    <!-- tour library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/bootstrap-tour.js"></script>
    <!-- library for cookie management -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.cookie.js"></script>
    <!-- calander plugin -->
    <script src='/Content/Templates/usmanhalalit-charisma-d29440e/js/fullcalendar.min.js'></script>
    <!-- data table plugin -->
    <script src='/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.dataTables.min.js'></script>
    <!-- chart libraries start -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/excanvas.js"></script>
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.flot.min.js"></script>
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.flot.pie.min.js"></script>
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.flot.stack.js"></script>
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.flot.resize.min.js"></script>
    <!-- chart libraries end -->
    <!-- select or dropdown enhancer -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.chosen.min.js"></script>
 @*   <!-- checkbox, radio, and file input styler -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.uniform.min.js"></script>*@
    <!-- plugin for gallery image view -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.colorbox.min.js"></script>
    <!-- rich text editor library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.cleditor.min.js"></script>
    <!-- notification plugin -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.noty.js"></script>
    <!-- file manager library -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.elfinder.min.js"></script>
    <!-- star rating plugin -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.raty.min.js"></script>
    <!-- for iOS style toggle switch -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.iphone.toggle.js"></script>
    <!-- autogrowing textarea plugin -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.autogrow-textarea.js"></script>
    <!-- multiple file upload plugin -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.uploadify-3.1.min.js"></script>
    <!-- history.js for cross-browser state change on ajax -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/jquery.history.js"></script>
    <!-- application script for Charisma demo -->
    <script src="/Content/Templates/usmanhalalit-charisma-d29440e/js/charisma.js" type="text/javascript"></script>
</body>
</html>
 @RenderSection("root", false)
 @TempData[NFD.Entities.Common.PubConst.VIEW_DATA_JS]